{
    "run_time_days": 2,
    "log_to_postgres": 0,
    "console_log_level": 20,
    "file_log_level": 20,
    "pg_log_level": 10,
    "devices": {
        "grid_controllers": [
            {
                "device_id": "gc_1",
                "uuid": 1, 
                "device_type": "grid_controller",
                "static_price": false,
                "threshold_alloc": 0.75, 
                "message_latency": 1,  
                "price_logic": "weighted_average",
                "connected_devices": ["utm_1", "eud_1", "eud_2", "pv_1"], 
                "battery": {
                    "battery_id": "battery_1",
                    "device_type": "battery",
                    "capacity": 50000.0,
                    "price_logic": "hourly_preference", 
                    "starting soc": 0.5 
                }
            }
        ],
        "utility_meters": [
            {
                "device_id": "utm_1",
                "uuid": 2,
                "message_latency": 1, 
                "capacity": 5000.0,
                "schedule": [
                    [0, "turn_on"]
                ],
                "price_schedule": [
                    [0, 0.10], [8, 0.15], [18, 0.05], [24, 0.1], [32, 0.15], [42, 0.05]
                ]
            }
        ],
        "pvs":[
            {
                "device_id": "pv_1",
                "uuid": 4,
                "message_latency": 1, 
                "peak_power": 1800.0,
                "data_filename": "pv_data_winter.csv"
            }
        ], 
        "euds": [
            {
                "device_id": "eud_1",
                "uuid": 3,
                "eud_type": "light",
                "max_operating_power": 500.0, 
                "message_latency": 1, 
                "modulation_interval": 7200,            
                "schedule": [
                    [0, "turn_on"], [6, "turn_off"], [8, "turn_on"], [18, "turn_off"], [24, "turn_on"], [30, "turn_off"], [32, "turn_on"], [42, "turn_off"]
                ]
            }, 
            {
                "device_id": "eud_2",
                "uuid": 5,
                "eud_type": "air_conditioner",
                "message_latency": 1,  
                "compressor_operating_power": 500.0, 
                "initial_temp": 25.0,
                "temp_max_delta": 0.5,
                "initial_set_point": 23.0,
                "price_to_setpoint": [[0.05, 18.0], [0.10, 22.0], [0.15, 30.0]], 
                "precooling_enabled": false,
                "compresor_cooling_rate": 2.0,
                "heat_exchange_rate": 0.1,
                "modulation_interval": 600,
                "temperature_update_interval": 300,
                "schedule": [[0, "turn_on"]], 
                "external_data":{
                    "temperature_schedule": {
                        "readin_function": "read_air_conditioner_data",
                        "source_file": "weather_5_secs.csv"
                    }
                }
            }
        ]
    }
}